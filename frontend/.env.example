# Supabase Configuration - REQUIRED
# Replace with your actual Supabase URL from your project dashboard
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co

# Replace with your actual anon key from your Supabase project
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# API Configuration - REQUIRED
# URL for your backend API
NEXT_PUBLIC_API_URL=http://localhost:5001/api

# Render Deployment - Optional, only needed for CI/CD with Render
# Get your Render API key from the Render dashboard
RENDER_API_KEY=your-render-api-key

# Get your Render service ID from the service settings page
RENDER_SERVICE_ID=your-render-service-id

# Vercel Deployment - Optional, only needed for CI/CD with Vercel
# Name of your Vercel project
VERCEL_PROJECT_NAME=your-project-name

# Team ID for your Vercel organization
VERCEL_TEAM_ID=your-team-id 